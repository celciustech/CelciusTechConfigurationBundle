    "autoload" : {
        "psr-0" : {
            "CelciusTech\\CoreBundle" : ""
        }
    },
    "target-dir" : "CelciusTech/CoreBundle",
    "repositories" : [
        {
          "type" : "vcs",
          "url" : "https://github.com/celciustech/CelciusTechCoreBundle.git"
        },
    ],
    "extra" : {
        "branch-alias" : {
            "dev-master" : "v1.0.0"
        }
    }
