{
    "name" : "celciustech/configuration-bundle",
    "description" : "Configuration for your applications",
    "type" : "symfony-bundle",
    "authors" : [{
        "name" : "Welly",
        "email" : "welly@celciustech.net"
    }],
    "keywords" : [
        "ConfigurationBundle"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.3",
        "celciustech/core-bundle": "dev-master"
